<UserControl x:Class="BasaDann.Views.HelpView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BasaDann.Views"
             xmlns:viewModel="clr-namespace:BasaDann.ViewModels"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Image Source="{Binding Curr_Im}"></Image>
        <Button Width="60" Height="20" HorizontalAlignment="Right" 
                Margin="35" Content="Дальше" Command="{Binding Image}"/>
    </Grid>
</UserControl>
